<%- include('../partials/header') %>


<h1> Welcome to Musical Chairs </h1>

<table id="list">
    <thead>
      <tr>
        <th>Playlist Name</th>
        <th>Genre</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% playlists.forEach(function(p) { %>
        <tr>
          <td><%= p.playlistName %></td>
          <td><%= p.genre %></td>
          <td><a href="/playlists/<%= p._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>

<%- include('../partials/footer') %>